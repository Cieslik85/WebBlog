<%- include ("partials/header.ejs"); %>

<section>
  <div class="top-container">
    <% artList.forEach((art, index) => { %>
      
      <div class="card box-shadow" id="<%= index %>">
        
        <h2 class="title"><%= art.title %></h2>
    
        <p><%= art.content %></p>
        <div class="date">
          <p><%= art.date %></p>
        </div>
        

      </div>
    
    <% }); %>
  </div>
</section>


<%- include("partials/footer.ejs") %>